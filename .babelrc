{
  "comments": false,
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": { "node": "current" }
          }
        ]
      ],
      "plugins": [["istanbul"], ["transform-decorators-legacy"]]
    },
    "main": {
      "presets": [
        [
          "env",
          {
            "targets": { "node": "current" }
          }
        ]
      ],
      "plugins": ["transform-decorators-legacy"]
    },
    "renderer": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ]
      ],
      "plugins": ["transform-decorators-legacy"]
    },
    "translator": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ]
      ],
      "plugins": ["transform-decorators-legacy"]
    },
    "web": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ]
      ],
      "plugins": ["transform-decorators-legacy"]
    }
  }
}
